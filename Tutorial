SELECT * FROM users;

SELECT username, email FROM users;

SELECT * FROM users 
WHERE username = 'john_doe';

SELECT * FROM users 
ORDER BY created_at DESC;

UPDATE users 
SET email = 'john.updated@example.com' 
WHERE username = 'john_doe';

UPDATE users 
SET email = 'jane.updated@example.com', username = 'jane_updated' 
WHERE user_id = 2;









